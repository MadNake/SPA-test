{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-extra-semi": 0,
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always"
            }
         ],
         "import/prefer-default-export": 0
    }
}
